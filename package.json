{
  "name": "sessions-provider-memory",
  "version": "0.1.2",
  "description": "Sessions Memory Provider",
  "license": "MIT",
  "repository": "trekjs/sessions-provider-memory",
  "author": {
    "name": "Fangdun Cai",
    "email": "cfddream@gmail.com",
    "url": "trekjs.com"
  },
  "keywords": [
    "trek",
    "sessions",
    "provider",
    "memory"
  ],
  "engines": {
    "node": ">= 6.0.0"
  },
  "files": [
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "xo",
    "precommit": "lint-staged",
    "test": "xo && nyc ava"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "eslint-config-prettier": "2.4.0",
    "husky": "0.14.3",
    "lint-staged": "4.1.3",
    "nyc": "11.2.1",
    "prettier": "1.6.1",
    "request": "2.81.0",
    "request-promise": "4.2.1",
